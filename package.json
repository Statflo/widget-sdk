{
  "name": "@statflo/widget-sdk",
  "version": "0.3.2",
  "description": "SDK for building widgets within Textkit and beyond",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Statflo Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Statflo/widget-sdk.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Statflo/widget-sdk/issues"
  },
  "homepage": "https://github.com/Statflo/widget-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "browserify": "^17.0.0",
    "domino": "^2.1.6",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-import": "^2.25.4",
    "http-server": "^14.1.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rollup": "^2.69.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start:gh-pages": "cd ./docs && http-server",
    "test": "jest --config jestconfig.json --passWithNoTests",
    "build": "tsc",
    "build:gh-pages": "yarn build && cp -r ./dist ./docs && browserify ./docs/main.js --standalone WidgetSDK > ./docs/bundle.js",
    "lint": "yarn eslint . --ext .ts --quiet",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "test-coverage": "yarn test --coverage"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "iframe-resizer": "^4.3.2"
  }
}
