{
  "name": "@statflo/widget-sdk",
  "version": "0.1.1",
  "description": "SDK for building widgets within Textkit and beyond",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Statflo Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Statflo/widget-sdk.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Statflo/widget-sdk/issues"
  },
  "homepage": "https://github.com/Statflo/widget-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "domino": "^2.1.6",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "test": "jest --config jestconfig.json --passWithNoTests",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
}